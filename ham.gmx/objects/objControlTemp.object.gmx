        <arguments>
          <argument>
            <kind>1</kind>
            <string>t++

if t == 1 {
    audio_play_sound(choose(sndSignalLaunch, sndSignalLaunch2), 0.5, false)
}

if t == 120 {
    instance_create(objRadioTower.x, objRadioTower.y, objPlayer)
}

if t == 130 {
    instance_create(0, 0, objCamera)
}

if t == 250 {
    objSpawner.disabled = false
}

if instance_exists(objPlayer) and objPlayer.state != 2 {

    // Add score based on time
    if t &gt; 120 and t mod 60 == 0 {
        roundScore ++
    }

    // Ending
    
    var endTime = 4000 // 6000
    
    if t == endTime {
        objSpawner.disabled = true
    }
    
    if t == endTime+160 {
        objPlayer.state = 1
        
        instance_create(room_width/2+80, view_yview[0] - 1000, objAlienRadioTower)
        
        var m = instance_create(0, view_yview[0] - 1500, objMoon);
        m.image_yscale = -1
        
        var a = instance_create(150, view_yview[0] - 1300, objAlien);
        a.image_angle = 180
    }

    // God intermission
    
    var godTime = 2000 // 3000
    
    if t == godTime-30 {
        objSpawner.disabled = true
    }
    
    if t == godTime+200 {
        objSpawner.disabled = false
    }
    
    if t == godTime-20 {
        instance_create(room_width, view_yview[0]-500, objGod)
        
        with instance_create(room_width/2-50, view_yview[0]-550, objHalfway) {
            image_xscale = 2
            image_yscale = image_xscale
        }
        
        // Spawn clouds
        var n = 50
        for (var i=0; i&lt;n; i++) {
        
            // X is random
            var xx = random_range(-50, room_width+50)
            // Y increases linearly
            var yy = view_yview[0]-500 + lerp(-400, 400, i/n)
            
            var scale = choose(2,3)
            // larger clouds toward the middle
            if i &gt; 10 and i &lt; n-10 {
                scale *= 2
            }
            
            with instance_create(xx, yy, objGodCloud) {
                image_xscale = scale*choose(1, -1)
                image_yscale = scale
                
                hspeed = random_range(-1, 1)
                
                image_alpha = random_range(0.8, 1)
            }
        }
    }

}

// temp restart game
if keyboard_check_pressed(ord('R')) {
    audio_stop_sound(sndSpace)
    room_goto(room)
}

// temp exit game
if keyboard_check_pressed(vk_escape) {
    game_end()
}
</string>
